<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="dataConfiguration" type="Microsoft.Practices.EnterpriseLibrary.Data.Configuration.DatabaseSettings, Microsoft.Practices.EnterpriseLibrary.Data, Version=4.1.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" />
    <section name="cachingConfiguration" type="Microsoft.Practices.EnterpriseLibrary.Caching.Configuration.CacheManagerSettings, Microsoft.Practices.EnterpriseLibrary.Caching, Version=4.1.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" />
  </configSections>
  <dataConfiguration defaultDatabase="CormitRouteTrackingBusinessInterface" />
  <cachingConfiguration defaultCacheManager="Cache Manager">
    <cacheManagers>
      <add name="Cache Manager" expirationPollFrequencyInSeconds="60" maximumElementsInCacheBeforeScavenging="1000" numberToRemoveWhenScavenging="10" backingStoreName="Null Storage" type="Microsoft.Practices.EnterpriseLibrary.Caching.CacheManager, Microsoft.Practices.EnterpriseLibrary.Caching, Version=4.1.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" />
    </cacheManagers>
    <backingStores>
      <add name="Null Storage" encryptionProviderName="" type="Microsoft.Practices.EnterpriseLibrary.Caching.BackingStoreImplementations.NullBackingStore, Microsoft.Practices.EnterpriseLibrary.Caching, Version=4.1.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" />
    </backingStores>
  </cachingConfiguration>
  <connectionStrings>
    <add name="CormitRouteTrackingBusinessInterface" connectionString="Integrated Security=False;Persist Security Info=False;Data Source=#{SqlInstance};User ID=#{i360_application_usr};Password=#{i360_application_pwd};Initial Catalog=Imarda360.Tracking;Min Pool Size=20;Max Pool Size=200" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <add key="DateGenerated" value="2014-11-12T18:09:05" />
    <add key="LoggingServiceUrl" value="#{LoggingEndpointAddress}" />
    <add key="LogToDatabase" value="1" />
    <add key="MaxLogLevel" value="3" />
    <add key="RSLBatchSize" value="100" />
    <add key="RSLLowestSpeedLimit" value="30" />
    <add key="RslProbabilityThreshold" value="0" />
    <add key="RSLRunInterval" value="1000" />
    <add key="RSLSpeedCheckEnabled" value="true" />
  </appSettings>
  <system.serviceModel>
    <services>
      <service name="CormitRouteTrackingBusiness.CormitRouteTracking">
        <endpoint name="TrackingTcpEndpoint" address="#{TrackingBusinessEndpointAddress}" binding="netTcpBinding" bindingConfiguration="DefaultBinding" contract="CormitRouteTrackingBusiness.ICormitRouteTracking" behaviorConfiguration="DefaultBehavior" />
      </service>
    </services>
    <client>
      <endpoint name="GISTcpEndpoint" address="#{GISBusinessEndpointAddress}" binding="netTcpBinding" bindingConfiguration="DefaultBinding" contract="ImardaGISBusiness.IImardaGIS" behaviorConfiguration="DefaultBehavior" />
    </client>
    <bindings>
      <netTcpBinding>
        <binding name="DefaultBinding" closeTimeout="00:00:10" receiveTimeout="00:05:00" sendTimeout="00:01:00" maxBufferPoolSize="524288" maxBufferSize="104857600" maxReceivedMessageSize="104857600" maxConnections="500">
          <readerQuotas maxStringContentLength="104857600" />
          <security mode="None">
            <transport protectionLevel="None" />
          </security>
        </binding>
      </netTcpBinding>
    </bindings>
    <behaviors>
      <serviceBehaviors>
        <behavior name="debugTrue">
          <serviceDebug includeExceptionDetailInFaults="true" />
          <serviceThrottling maxConcurrentCalls="250" maxConcurrentSessions="250" maxConcurrentInstances="1" />
        </behavior>
      </serviceBehaviors>
      <endpointBehaviors>
        <behavior name="DefaultBehavior">
          <dataContractSerializer maxItemsInObjectGraph="16777216" />
        </behavior>
      </endpointBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>